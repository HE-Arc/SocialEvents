<h1>Main</h1>

<h2>Top contributeurs</h2>

<% @users_top.each do |user| %>
<p><%= user.first_name %> <%= user.last_name %> <%= user.contributions_counter %></p>
<% end %>

<h2>Evénements</h2>

<% @events.each do |event| %>
<p><%= link_to event.title, event %> <%= event.category %></p>
<% end %>

<h2>Catégories</h2>

<% @categories.each do |category| %>
<p><%= category %>
<% end %>
  
<h2>Cantons</h2>

<% @cantons.each do |canton| %>
<p><%= canton %>
<% end %>